@model IEnumerable<BankDataApp.Models.Detail>
<h1>Detailssss</h1>
<div class="row">
    <table class="table table-striped table-bordered dt-responsive nowrap" width="100%" cellspacing="0">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.TransactionDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Amount)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Status)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.EffectiveStatusDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.TimeBreached)
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.TransactionDate)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Amount)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Status)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.EffectiveStatusDate)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.TimeBreached)
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>